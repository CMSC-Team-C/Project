        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/eggthree"
        android:shadowColor="#FFFFFF"
        android:shadowDx="5.0"
        android:shadowDy="5.0"
        android:shadowRadius="1.0"
